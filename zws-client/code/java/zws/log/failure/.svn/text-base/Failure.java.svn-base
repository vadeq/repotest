import zws.log.MessageBase;

public class Failure extends MessageBase {
  public Failure(String messageKey) { super(messageKey); }
  public Failure(Exception e) { super(e); }
  public Failure(String messageKey, Exception e) { super(messageKey, e); }
  public Failure(String messageKey, String arg1, Exception e) { super(messageKey, arg1, e); }
  public Failure(String messageKey, String arg1, String arg2, Exception e) { super(messageKey, arg1, arg2, e); }
}
