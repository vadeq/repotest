    <menu-item image="publish_pending_nav" tip="PEN Publishing: Pending Review" url="/publish/cisco.do?menuItem=0.2&amp;publishPending=true" roles="all-roles"/>
    <menu-item image="publish_logs_nav" tip="PEN Status and Activity Monitor" url="/publish/retrieveque.do?menuItem=0.3" roles="all-roles"/>

<!--     <menu-item image="publish_logs_nav" tip="PEN Status and Activity Monitor" url="/pen/cisco-1.3.1.jsp?menuItem=0.3" roles="all-roles"/> -->
