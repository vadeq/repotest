import zws.event.hiEventBase;

public class ReplicateDesign extends hiEventBase {

  public String getCriteria() { return getString(CRITERIA); }
  public void setCriteria(String s) { set(CRITERIA, s); }
  public String getPolicyServer() { return getString(POLICY_SERVER); }
  public void setPolicyServer(String s) { set(POLICY_SERVER, s); }
  public String getPolicyName() { return getString(POLICY_NAME); }
  public void setPolicyName(String s) { set(POLICY_NAME, s); }
  public String getConflictOeverwrites() { return getString(CONFLICT_OVERWRITES); }
  public void setConflictOverwrites(String s) { set(CONFLICT_OVERWRITES, s); }
  
  private static String CRITERIA = "criteria";
  private static String POLICY_NAME = "policy-name";
  private static String POLICY_SERVER = "policy-server";
  private static String CONFLICT_OVERWRITES = "conflict-overwrites";
}
