      <property name="model-check-notification-footer" value="This message generated by DesignState"/> 
      <property name="model-check-promotion-folder" value="/Root Folder/FORMS/RTP"/> 
      <property name="model-check-promotion-level" value="RELEASE"/> 
      <property name="model-check-attributes" value="Folder; Revision; Release-Level; Title"/>

<!-- Baselines -->
      <property name="baseline-default-release" value="RELEASED"/>
      <property name="promo-baseline-folder" value="Root Folder/Baselines" />

