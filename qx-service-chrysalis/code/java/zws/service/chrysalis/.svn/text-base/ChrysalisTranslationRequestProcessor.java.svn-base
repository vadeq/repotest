package zws.service.chrysalis;

import zws.exception.zwsException;
import zws.qx.QxContext;
import zws.qx.xml.QxXML;

public class ChrysalisTranslationRequestProcessor implements ChrysalisRequestProcessor {

  protected ChrysalisRequestProcessorResource resource;
  protected QxContext ctx;
  protected QxXML instr;
  
  public ChrysalisTranslationRequestProcessor(QxContext context, QxXML instructions) throws zwsException {
    resource = new ChrysalisRequestProcessorResource();
    ctx = context;
    instr = instructions;
  }

  public QxXML process() throws Exception {
    return new QxXML();
  }

}
