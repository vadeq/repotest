      <property name="publish-to-agile-notification-reply-to" value="admin@harris.com"/>
      <property name="publish-to-agile-notification-subject" value="ILink To Agile."/>
      <property name="publish-to-agile-notification-recipients" value="kcornish@harris.com"/> <!-- use; as delimiter for  addresses -->
      <property name="publish-to-agile-notification-header" value="The following parts have are being published to Agile:"/> 
