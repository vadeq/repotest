      <property name="central-server" value="DesignState-node-0" />
      <property name="server-list" value="DesignState-node-0" />
      <property name="pen-namespace" value="publishing.engine" />
      <property name="pen-name" value="transfer" />
    	<property name="domain-name" value="harris"/>
      <property name="start-page" value="publish-search" />
      <property name="DesignState-database" value="DesignState" />
