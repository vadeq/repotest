import zws.origin.Origin;

public class FailedToRetrieveMetadata extends Exception {
 private static String PREFIX_MSG = "Failed to retrieve metadata for ";
 public FailedToRetrieveMetadata(Origin o) { super(PREFIX_MSG + o.toString()); }
 public FailedToRetrieveMetadata(String s) { super(PREFIX_MSG + s); }
}