import zws.util.comparator.AlphaNumericComparator;

import java.io.Serializable;
import java.util.Comparator;

public class SortByKey implements Serializable, Comparator {
  public int compare (Object data0, Object data1) { return comparator.compare(data0, data1); }
  
  public boolean getAscendingOrder() { return comparator.getAscendingOrder(); }
  public void setAscendingOrder(boolean b) { comparator.setAscendingOrder(b); }
 
  protected AlphaNumericComparator comparator = new AlphaNumericComparator();
}
