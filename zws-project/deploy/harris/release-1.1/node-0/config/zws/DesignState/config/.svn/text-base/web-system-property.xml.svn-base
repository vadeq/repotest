<!-- report Configurations -->

	<!-- XSL STYLESHEETS  -->

	<dir name="zws-home" path="/zws"/>
	<dir name="zws-config" path="[zws-home]/DesignState/config"/>

<!-- MODULE SPECIFIC PROPERTIES -->
