import zws.event.*;

public class Renamed extends DesignEventBase  {
  public String getOldName() { return (String) getProperties().get(OLD_NAME); }
  public void setOldName(String s) { getProperties().put(OLD_NAME, s); }

  public String getNewName() { return (String) getProperties().get(NEW_NAME); }
  public void setNewName(String s) { getProperties().put(NEW_NAME, s); }

  private static String OLD_NAME="name";
  private static String NEW_NAME="new-name";
}
