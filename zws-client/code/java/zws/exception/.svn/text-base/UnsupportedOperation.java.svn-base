public class UnsupportedOperation extends RuntimeException {
    public UnsupportedOperation(String operation) {
    super(operation + " operation not supported");
  }
    public UnsupportedOperation(String operation, String hint) {
    super(operation + " operation not supported: " + hint);
  }
}