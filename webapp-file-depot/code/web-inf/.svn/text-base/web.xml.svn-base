<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

    <display-name>FileDepot</display-name>

    <servlet>
        <servlet-name>FileDepot</servlet-name>
        <servlet-class>zws.service.file.depot.FileDepot</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FileDepot</servlet-name>
        <url-pattern>/FileDepot</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>FileDepot</servlet-name>
        <url-pattern>/servlet/FileDepot</url-pattern>
    </servlet-mapping>
   
</web-app>