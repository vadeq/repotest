import zws.event.*;

public class Moved extends DesignEventBase  {
  public String getOldLocation() { return getString(OLD_FOLDER); }
  public void setOldLocation(String s) { set(OLD_FOLDER, s); }
  public String getNewLocation() { return getString(NEW_FOLDER); }
  public void setNewLocation(String s) { set(NEW_FOLDER, s); }
  
  private static String OLD_FOLDER = "folder";
  private static String NEW_FOLDER = "new-folder";
}
