import zws.util.RoutedEventBase;



/**
 * The Class Event.
 */
public class Event extends RoutedEventBase {

  /** The event. */
  private String event = null;

  /**
   * The Constructor.
   */
  public Event() {
    {} //System.out.println("Event constructor....");
  }

  /**
   * The Constructor.
   * @param event the event
   */
  public Event(String event) {
    super();
    this.event = event;
 }

  /**
   * Gets the event.
   *
   * @return the event
   */
  public String getEvent() {
    return event;
  }

  /**
   * Sets the event.
   *
   * @param event the event
   */
  public void setEvent(String event) {
    this.event = event;
    }
}
